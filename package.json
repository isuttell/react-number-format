    "start": "grunt server",
    "build": "grunt build",
    "test": "./node_modules/jest-cli/bin/jest.js src/ --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isuttell/react-number-format.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Isaac Suttell <isaac@isaacsuttell.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ship-components/react-number-format/issues"
  },
  "homepage": "https://github.com/ship-components/react-number-format#readme",
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-core": "^6.1.4",
    "babel-eslint": "^4.1.5",
    "babel-jest": "git+https://github.com/isuttell/babel-jest.git#master",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "coveralls": "^2.11.4",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-eslint": "^17.3.1",
    "grunt-webpack": "^1.0.11",
    "jest": "^0.1.40",
    "jest-cli": "^0.7.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.2",
    "react-addons-css-transition-group": "^0.14.2",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/classnames",
